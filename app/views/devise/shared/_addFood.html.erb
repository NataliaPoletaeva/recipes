<%= form_with(model: food) do |form| %>

  <div>
    <%= form.label :name %>
    <%= form.text_field :name %>
  </div>

  <div>
    <%= form.label :measurement_unit %>
    <%= form.text_field :measurement_unit %>
  </div>

  <div>
    <%= form.label :price %>
    <%= form.text_field :price %>
  </div>


  <div>
    <%= form.submit "Create Food" %>
  </div>
<% end %>